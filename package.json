{
  "name": "tokimeki-editing",
  "displayName": "TOKIMEKI Editing🌈",
  "description": "Insert emojis representing them in Markdown",
  "version": "0.0.2",
  "publisher": "everlasting-diary",
  "keywords": [
    "emoji",
    "lovelive",
    "nijigaku",
    "虹ヶ咲学園スクールアイドル同好会"
  ],
  "galleryBanner": {
    "color": "#1D1D1D",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ftnext/vscode-tokimeki-editing.git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
